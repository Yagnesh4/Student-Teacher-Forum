<body>


  <div class="wrapper">
    <div class="title-text">
       <div class="title signup">
          Detail's about <span>Teacher</span> 
       </div>
    </div>
    <div class="form-container">
       
       <div class="form-inner">
          
          <form class="signup" style="height: 500px;">
            <div class="field">
               Name:- <input  value="<%=data.name%>" readonly>
            </div><br>
            <div class="field">
               Gender:- <input  value="<%=data.gender%>" readonly>
            </div><br>
            <div class="field">
                Age:- <input  value="<%=data.age%>" readonly>
            </div><br>
            <div class="field">
                Email:- <input  value="<%=data.email%>" readonly>
            </div><br>
            <div class="field">
                Number:- <input  value="<%=data.number%>" readonly>
            </div> <br>
             </div>
          </form>
       </div>
    </div>
</div>

  
   
    <div id = "option" class="option container " style="display: block;">
        <div class="row fixed-content">
            <form class="signup" action="/view-teacher"  method="POST">   
                <div class=" option1" style="margin-left: 530px;">
                    <button id="accept" class="btn " >Accept</button>
                </div>
            </form>

            <form class="signup" action="/reject"  method="POST">  
                <div class=" option1" style="margin-left: 70px; ;">
                    <button id="reject" class="btn skip">Reject</button>
                </div>  
            </form>    
         
        </div>
      </div>

  
    



  <script src="css/bootstrap-4.0.0-dist/js/jquery-3.5.1.min.js"></script>
  <script src="css/bootstrap-4.0.0-dist/js/bootstrap.min.js"></script>
  <script>
    $('#reject').click(function(){
     window.location.reload(true) = "view-teacher";
   })
 </script>

    
</body>
